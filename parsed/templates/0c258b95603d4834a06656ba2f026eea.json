{
  "components/custom/main-menu/header": "<header class=\"header-custom\" role=\"banner\">\n  <a href=\"#\" class=\"mobileMenu-toggle\">\n    <span class=\"mobileMenu-toggleIcon\"></span>\n  </a>\n  <div class=\"top-bar\">{{> components/custom/main-menu/navigation}}</div>\n\n  <div class=\"header-logo--wrap\">\n    <div\n      class=\"header-logo {{#if theme_settings.logo_size '===' 'original'}}logo-full-size{{/if}}\"\n    >\n      <div class=\"logo-and-toggle\">\n        {{#if page_type '===' 'default'}}\n        <h1 class=\"logo-wrap\">{{> components/custom/main-menu/store-logo}}</h1>\n        {{else}}\n        <div class=\"logo-wrap\">\n          {{> components/custom/main-menu/store-logo}}\n        </div>\n        {{/if}}\n      </div>\n      <div class=\"navPages-navigationMenu\" id=\"menu\" data-menu>\n        {{> components/common/navigation-menu}}\n      </div>\n      <div>\n        <a href=\"#\" data-search=\"quickSearch\" aria-controls=\"quickSearch\" aria-expanded=\"false\">\n          <i class=\"icon\" aria-hidden=\"true\">\n            <svg><use xlink:href=\"#icon-search\" /></svg> </i\n        ></a>\n        \n      </div>\n      <div>\n        <a\n          class=\"navUser-action\"\n          data-cart-preview\n          data-dropdown=\"cart-preview-dropdown\"\n          data-options=\"align:right\"\n          href=\"{{urls.cart}}\"\n        >\n          <i class=\"icon\" aria-hidden=\"true\">\n            <svg><use xlink:href=\"#icon-cart\" /></svg>\n          </i>\n          (<span\n            class=\"{{#if cart.items}} countPill--positive{{/if}} cart-quantity\"\n            >{{cart.quantity}}</span\n          >)\n        </a>\n\n        <div\n          class=\"dropdown-menu\"\n          id=\"cart-preview-dropdown\"\n          data-dropdown-content\n          aria-hidden=\"true\"\n        ></div>\n      </div>\n    </div>\n  </div>\n  <div class=\"navPages-navigationMenu\" id=\"mobileMenu\" data-menu>\n    {{> components/custom/main-menu/navigation-menu-mobile}}\n  </div>\n</header>\n{{#unless page_type '===' 'default'}} {{#if banners.top}}\n<div class=\"banners\" data-banner-location=\"top\">\n  {{#each (limit banners.top_metadata 1)}}\n  <div\n    class=\"banner\"\n    data-event-type=\"promotion\"\n    data-entity-id=\"{{this.id}}\"\n    data-name=\"{{this.banner-name}}\"\n    data-position=\"{{this.location}}\"\n    data-banner-id=\"{{this.banner-name}}\"\n  >\n    <div data-event-type=\"promotion-click\">{{{this.content}}}</div>\n  </div>\n  {{/each}}\n</div>\n{{/if}} {{/unless}} {{{region name=\"header_bottom\"}}}\n\n<div class=\"dropdown dropdown--quickSearch\" id=\"quickSearch\" aria-hidden=\"true\" tabindex=\"-1\" data-prevent-quick-search-close>\n  {{> components/custom/main-menu/quick-search}}\n</div>\n\n<script>\n  $(document).ready(function(){\n  $(\".mobileMenu-toggle\").click(function(){\n    $(\"#mobileMenu\").toggleClass(\"mobileMenuIsOpen\");\n    $(\".navPagesMobile\").toggleClass(\"navPagesIsOpen\");\n  });\n});\n</script>\n",
  "components/custom/main-menu/navigation-menu-mobile": "\r\n<nav class=\"navPagesMobile\">\r\n    <ul class=\"navPages-list navPages-list--user 5765697a656e20596f756e67\">\r\n        {{#if customer}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action\" href=\"/account.php\" data-collapsible=\"navPages-account\" style=\"color:#000;\">\r\n                    {{lang 'common.account'}}\r\n            </li>\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action\" href=\"{{urls.auth.logout}}\" style=\"color:#000;\">{{lang 'common.logout'}}</a>\r\n            </li>\r\n        {{else}}\r\n            <li class=\"navPages-item\">\r\n                <i class=\"icon\" aria-hidden=\"true\">\r\n                    <svg><use xlink:href=\"#icon-user\" /></svg>\r\n                </i>\r\n                <a class=\"navPages-action\" href=\"{{urls.auth.login}}\">{{lang 'common.login'}}</a>\r\n                {{#if settings.account_creation_enabled}}\r\n                    {{lang 'common.or'}} <a class=\"navPages-action\" href=\"{{urls.auth.create_account}}\">{{lang 'common.sign_up'}}</a>\r\n                {{/if}}\r\n            </li>\r\n        {{/if}}\r\n        {{#if currency_selector.currencies.length '>' 1}}\r\n            <li class=\"navPages-item navPages-item--currency\">\r\n                <a class=\"navPages-action has-subMenu\" href=\"#\" data-collapsible=\"navPages-currency\" aria-controls=\"navPages-currency\" aria-expanded=\"false\">\r\n                    {{lang 'common.currency' code=currency_selector.active_currency_code}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-down\"></use></svg></i>\r\n                </a>\r\n                <div class=\"navPage-subMenu\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\r\n                    <ul class=\"navPage-subMenu-list\">\r\n                        {{#each currency_selector.currencies}}\r\n                        <li class=\"navPage-subMenu-item\">\r\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{{switch_url}}}\">\r\n                                {{#if is_active}}\r\n                                    <strong>{{name}}</strong>\r\n                                {{else}}\r\n                                    {{name}}\r\n                                {{/if}}\r\n                            </a>\r\n                        </li>\r\n                        {{/each}}\r\n                    </ul>\r\n                </div>\r\n            </li>\r\n        {{/if}}\r\n    </ul>\r\n    <ul class=\"navPages-list navPages-mega navPages-mainNav\">\r\n        \r\n        {{#if theme_settings.show_content_navigation}}\r\n            {{#each pages}}\r\n                <li class=\"navPages-item{{#if @index '===' 0}} navPages-item--content-first{{/if}}\">\r\n                    {{#if ../theme_settings.navigation_design '===' 'simple'}}\r\n                        {{> components/common/navigation-list-alternate}}\r\n                    {{/if}}\r\n                    {{#if ../theme_settings.navigation_design '===' 'mega'}}\r\n                        {{> components/common/navigation-list}}\r\n                    {{/if}}\r\n                </li>\r\n             {{/each}}\r\n        {{/if}}\r\n        {{#each categories}}\r\n            <li class=\"navPages-item\" id=\"mobile-shop-menu\">\r\n                {{> components/common/navigation-list}}\r\n            </li>\r\n        {{/each}}\r\n    </ul>\r\n    <ul class=\"navPages-list navPages-list--user\">\r\n        {{#and settings.phone_number theme_settings.header-phone}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action navUser-action--phone\" href=\"tel:{{settings.phone_number}}\"><i class=\"fa fa-phone\" aria-hidden=\"true\"></i>&nbsp;{{settings.phone_number}}</a>\r\n            </li>\r\n        {{/and}}\r\n        {{#if settings.gift_certificates_enabled}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action\" href=\"{{urls.gift_certificate.purchase}}\">{{lang 'common.gift_cert'}}</a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if customer.store_credit.value '>' 0}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action navPages-action--storeCredit\">\r\n                    {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if theme_settings.product_list_display_mode '==' 'list'}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action navPages-action--compare\" href=\"{{urls.compare}}\" data-compare-nav>{{lang 'common.compare'}} <span class=\"countPill countPill--positive countPill--alt\"></span></a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if theme_settings.social_icon_placement_top}}\r\n            <li class=\"navPages-item\">\r\n                {{> components/common/social-links}}\r\n            </li>\r\n        {{/if}}\r\n    </ul>\r\n    <ul class=\"navPages-list navPages-list--user\" style=\"padding-top:15px;\">\r\n        <li class=\"navPages-item\"><a class=\"navUser-action\" href=\"tel:{{settings.phone_number}}\" style=\"color:#000;\">{{settings.phone_number}}</a></li>\r\n         <li class=\"navPages-item\" style=\"color:#000;\">55 Hilton St, Easton, PA 18042</li>\r\n     </ul>\r\n</nav>\r\n",
  "components/custom/main-menu/navigation": "<nav class=\"navUser\">\r\n    <ul class=\"navUser-section\">\r\n       <li class=\"navUser-item navUser-item--account\">Call Us at <a class=\"navUser-action\" href=\"tel:{{settings.phone_number}}\">{{settings.phone_number}}</a></li>\r\n        <li class=\"navUser-item navUser-item--account\">55 Hilton St, Easton, PA 18042</li>\r\n    </ul>\r\n    \r\n  <ul class=\"navUser-section navUser-section--alt\">\r\n    {{#if customer}}\r\n        <li class=\"navUser-item navUser-item--account\">\r\n        <a class=\"navUser-action\" href=\"{{urls.account.index}}\"\r\n            >{{lang 'common.account'}}</a\r\n        >\r\n        </li>\r\n        <li class=\"navUser-item navUser-item--account\">\r\n        <a class=\"navUser-action\" href=\"{{urls.auth.logout}}\"\r\n            >{{lang 'common.logout'}}</a\r\n        >\r\n        </li>\r\n    {{else}}\r\n        <li class=\"navUser-item navUser-item--account\">\r\n        <a class=\"navUser-action\" href=\"{{urls.auth.login}}\"\r\n            >{{lang 'common.login'}}</a\r\n        >\r\n        </li>\r\n            {{#if settings.account_creation_enabled}}\r\n            <li class=\"navUser-item navUser-item--account\">\r\n            <a class=\"navUser-action\" href=\"{{urls.auth.create_account}}\"\r\n                >{{lang 'common.sign_up'}}</a\r\n            >\r\n            </li>\r\n            {{/if}}\r\n    {{/if}}\r\n    \r\n    <li class=\"navUser-item navUser-item--account\">\r\n      <a class=\"button button--primary button--small\" href=\"/products/\" style=\"margin-bottom: 0; font-size: 13px;\">Shop Products</a>\r\n    </li>\r\n    <!--<li class=\"navUser-item navUser-item--cart\">\r\n    </li>-->\r\n  </ul>\r\n</nav>\r\n",
  "components/custom/main-menu/store-logo": "<a href=\"{{urls.home}}\">\n    {{#if settings.store_logo.image}}\n        <div class=\"header-logo-image-container\">\n            <img class=\"header-logo-image\" src=\"{{cdn 'icons/polytek-logo.svg'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n        </div>\n    {{else}}\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n",
  "components/custom/main-menu/quick-search": "<div>\n{{{ snippet \"forms_search\"}}}\n<a class=\"modal-close mobileOnly\" aria-label=\"{{lang 'common.close'}}\" data-drop-down-close role=\"button\">\n    <span aria-hidden=\"true\">&#215;</span>\n</a>\n<form class=\"form\" action=\"{{urls.search}}\">\n    <fieldset class=\"form-fieldset\">\n        <div class=\"form-field\">\n            <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\n            <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\"\n                {{#if forms.search.query}} value=\"{{forms.search.query}}\"{{/if}}>\n        </div>\n    </fieldset>\n</form>\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\n</div>\n",
  "components/common/navigation-menu": "<span class=\"mobile-closeNav\">\n    <i class=\"icon\" aria-hidden=\"true\">\n        <svg><use xlink:href=\"#icon-close-large\" /></svg>\n    </i>\n    close\n</span>\n<nav class=\"navPages\">\n    <ul class=\"navPages-list navPages-list--user 5765697a656e20596f756e67\">\n        {{#if currency_selector.currencies.length '>' 1}}\n            <li class=\"navPages-item navPages-item--currency\">\n                <a class=\"navPages-action has-subMenu\" href=\"#\" data-collapsible=\"navPages-currency\" aria-controls=\"navPages-currency\" aria-expanded=\"false\">\n                    {{lang 'common.currency' code=currency_selector.active_currency_code}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-down\"></use></svg></i>\n                </a>\n                <div class=\"navPage-subMenu\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPage-subMenu-list\">\n                        {{#each currency_selector.currencies}}\n                        <li class=\"navPage-subMenu-item\">\n                            <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-right\" /></svg></i>\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{{switch_url}}}\">\n                                {{#if is_active}}\n                                    <strong>{{name}}</strong>\n                                {{else}}\n                                    {{name}}\n                                {{/if}}\n                            </a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                </div>\n            </li>\n        {{/if}}\n        {{#if customer}}\n            <li class=\"navPages-item navPages-item--account\">\n                <a class=\"navPages-action has-subMenu\" href=\"{{urls.account.index}}\" data-collapsible=\"navPages-account\">\n                    <i class=\"icon\" aria-hidden=\"true\">\n                        <svg><use xlink:href=\"#icon-user\" /></svg>\n                    </i>\n                    &nbsp;\n                    {{lang 'common.account'}}\n                    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n                </a>\n                <div class=\"navPage-subMenu\" id=\"navPages-account\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPage-subMenu-list\">\n                        <li class=\"navPage-subMenu-item\">\n                            <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-right\" /></svg></i>\n                            <a class=\"navPage-subMenu-action navPages-action\"\n                               href=\"{{urls.account.orders.all}}\"\n                               aria-label=\"{{lang 'account.nav.orders'}}\"\n                            >\n                                {{lang 'account.nav.orders'}}\n                            </a>\n                        </li>\n                        {{#if settings.returns_enabled}}\n                            <li class=\"navPage-subMenu-item\">\n                                <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-right\" /></svg></i>\n                                <a class=\"navPage-subMenu-action navPages-action\"\n                                   href=\"{{urls.account.returns}}\"\n                                   aria-label=\"{{lang 'account.nav.returns'}}\"\n                                >\n                                    {{lang 'account.nav.returns'}}\n                                </a>\n                            </li>\n                        {{/if}}\n                    